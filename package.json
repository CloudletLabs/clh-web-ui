{
  "name": "clh-web-ui",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "preinstall": "npm prune",
    "postinstall": "bower prune && bower install",
    "clean": "rm -rf build",
    "prebuild": "npm run clean && npm install",
    "build": "grunt",
    "prestart": "npm run clean && npm install",
    "start": "grunt start",
    "pretest": "npm run clean && npm install && grunt bower",
    "test": "npm run test-only",
    "test-only": "karma start karma.conf.js",
    "pree2e": "npm run clean && npm install && webdriver-manager update",
    "e2e": "npm run e2e-only",
    "e2e-only": "grunt e2e"
  },
  "devDependencies": {
    "jade": "=1.11.0",
    "grunt": "=1.0.1",
    "grunt-contrib-copy": "=1.0.0",
    "grunt-contrib-connect": "=1.0.2",
    "grunt-contrib-jade": "=1.0.0",
    "grunt-contrib-watch": "=1.0.0",
    "grunt-contrib-uglify": "=2.0.0",
    "bower": "=1.7.9",
    "grunt-bower": "=0.21.2",
    "jasmine-core": "=2.5.2",
    "karma": "=1.3.0",
    "karma-requirejs": "=1.1.0",
    "karma-jasmine": "=1.0.2",
    "karma-phantomjs-launcher": "=1.0.2",
    "karma-coverage": "=1.1.1",
    "karma-junit-reporter": "=1.1.0",
    "phantomjs-prebuilt": "=2.1.13",
    "protractor": "=4.0.13",
    "cucumber": "=1.3.1",
    "grunt-protractor-webdriver": "=0.2.5",
    "grunt-protractor-runner": "=4.0.0",
    "protractor-jasmine-cucumber": "=0.2.2"
  }
}
