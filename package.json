    "grunt-contrib-jade": "=1.0.0",
    "grunt-contrib-watch": "=1.0.0",
    "grunt-contrib-uglify": "=2.0.0",
    "bower": "=1.7.9",
    "grunt-bower": "=0.21.2",
    "jasmine-core": "=2.5.2",
    "karma": "=1.3.0",
    "karma-requirejs": "=1.1.0",
    "karma-jasmine": "=1.0.2",
    "karma-phantomjs-launcher": "=1.0.2",
    "karma-junit-reporter": "=1.1.0",
    "phantomjs-prebuilt": "=2.1.13"
